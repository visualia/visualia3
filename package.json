{
  "name": "visualia3",
  "version": "0.0.1",
  "engines": {
    "node": "^14.13.1 || >=16.0.0"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/visualia/visualia3.git"
  },
  "homepage": "https://github.com/visualia/visualia3",
  "keywords": [
    "vite",
    "vue",
    "vue-component"
  ],
  "files": [
    "dist",
    "src"
  ],
  "main": "./dist/visualia.cjs.js",
  "module": "./dist/visualia.js",
  "exports": {
    ".": {
      "import": "./dist/visualia.js",
      "require": "./dist/visualia.cjs.js"
    }
  },
  "typings": "./dist/lib.es.ts",
  "scripts": {
    "dev": "vite --port 3000 --host",
    "build": "vite build && vite build --mode es && vite build --mode cjs",
    "preview": "vite preview --port 5000",
    "docs:dev": "vitepress dev docs --port 3001 --host",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress serve docs --port 5001",
    "test": "vitest",
    "typecheck": "vue-tsc --noEmit",
    "prerelease": "vitest --run && npm run build && vue-tsc --declaration --emitDeclarationOnly"
  },
  "dependencies": {
    "katex": "^0.15.1",
    "vue": "^3.2.25"
  },
  "devDependencies": {
    "@types/katex": "^0.11.1",
    "@types/node": "^17.0.2",
    "@types/react": "file:src/types",
    "@vitejs/plugin-vue": "^2.0.0",
    "typescript": "^4.4.4",
    "vite": "^2.7.2",
    "vitepress": "^0.20.9",
    "vitest": "^0.0.107",
    "vue-tsc": "^0.29.8"
  }
}
